import { from, of } from 'rxjs';

// const a = of('a', 'b', 'c');
// a.subscribe((x) => console.log(x));

// console.log('***********');
// const b = from([1, 2, 4]);
// b.subscribe((x) => console.log(x));

console.log('Difference between of & from operator');
let cars = ['A', 'B', 'C'];

from(cars).subscribe((x) => console.log(x));

of(cars).subscribe((x) => console.log(x));
